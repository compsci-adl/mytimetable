		"typescript": "^5.2.2",
		"typescript-eslint": "^8.6.0",
		"vite": "^5.4.8",
		"vite-plugin-pwa": "^0.20.5",
		"vitest": "^2.1.1"
	},
	"lint-staged": {
		"*.{js,ts,tsx,css,md,cjs,mjs,json,html}": [
			"prettier --write"
		]
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
